public void deleteClientApps(String id, ClientAppsObj clientAppsObj) {
    ClientAppConfiguration entity = this.repo.findById(id)
        .orElseThrow(() -> new NoSuchElementException("Data not available to delete for id: " + id));

    if (!entity.getClientId().equals(clientAppsObj.getClientId())) {
        log.error("Unauthorized attempt to delete config for id: {}", id);
        throw new AccessDeniedException("Client ID mismatch. Deletion not allowed.");
    }

    this.repo.deleteById(id);
    log.info("ClientApp Config for id {} deleted successfully", id);
}