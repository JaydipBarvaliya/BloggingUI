@Test
public void updateConfig() throws Exception {
    String id = "20";

    ArchivalStaticMetadataObj obj = new ArchivalStaticMetadataObj();
    obj.setLobId("TestLob");
    obj.setLookupId("TestLkpId");
    obj.setLookupValue("TestLookupValue");
    obj.setStorageMetadataField("TestStorageMetadataFld");
    obj.setStorageFieldType("TestStorageFldType");

    ArchStaticMetadataLookup config = new ArchStaticMetadataLookup();
    config.setId(1L);
    config.setLob("TestLob");
    config.setLookupId("TestLkpId");
    config.setLookupValue("TestLookupValue");
    config.setArcStorageMetadataField("TestStorageMetadataFld");
    config.setArcStorageFieldType("TestStorageFldType");

    when(repo.findById(Long.valueOf(id))).thenReturn(Optional.of(config));
    when(repo.saveAndFlush(any(ArchStaticMetadataLookup.class))).thenAnswer(i -> i.getArgument(0));

    ArchStaticMetadataLookup result = archStaticMetadataService.updateConfig(id, obj);

    // Assert the update logic
    assertNotNull(result);
    assertEquals("TestLob", result.getLob());
    assertEquals("TestLkpId", result.getLookupId());
    assertEquals("TestLookupValue", result.getLookupValue());
    assertEquals("TestStorageMetadataFld", result.getArcStorageMetadataField());
    assertEquals("TestStorageFldType", result.getArcStorageFieldType());
}