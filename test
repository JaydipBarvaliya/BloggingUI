Map<String, Boolean> mismatches = getMismatches(clientAppsObj, entity);
for (Map.Entry<String, Boolean> entry : mismatches.entrySet()) {
    if (Boolean.FALSE.equals(entry.getValue())) {
        log.error("Deletion failed due to mismatch in field: {}", entry.getKey());
        throw new AccessDeniedException("Mismatch in: " + entry.getKey());
    }
}