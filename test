@Test
public void createArchivalArchivalStaticMetadata() {
    // Arrange
    ArchivalStaticMetadataObj input = new ArchivalStaticMetadataObj();
    input.setLobId("TestLob1");
    input.setLookupId("TestLkpId1");
    input.setLookupValue("TestLookupVal1");
    input.setStorageMetadataField("TestFld1");
    input.setStorageFieldType("Type1");

    ArchStaticMetadataLookupDto dto = ArchStaticMetadataLkpMapper.MAPPER.configToDto(input);
    ArchStaticMetadataLookup entity = EntityToDTOMapper.INSTANCE.toEntity(dto);

    when(repo.saveAndFlush(any(ArchStaticMetadataLookup.class))).thenReturn(entity);

    // Act
    ArchivalStaticMetadataObj result = archStaticMetadataService.createArchivalArchivalStaticMetadata(input);

    // Assert
    assertNotNull(result);
    assertEquals("TestLob1", result.getLobId());
    assertEquals("TestLkpId1", result.getLookupId());
    assertEquals("TestLookupVal1", result.getLookupValue());
    assertEquals("TestFld1", result.getStorageMetadataField());
    assertEquals("Type1", result.getStorageFieldType());
}