@Test
public void getArchivalStaticMetadata() {
    String id = "20";
    ArchStaticMetadataLookup obj = new ArchStaticMetadataLookup();
    obj.setLob("TestLob");
    obj.setArcStorageMetadataField("TestMetadataField");
    obj.setArcStorageFieldType("TestFieldType");
    obj.setLookupValue("TestLookupValue");
    obj.setLookupId("TestLkpId");

    when(repo.findById(Long.valueOf(id))).thenReturn(Optional.of(obj));

    ArchivalStaticMetadataObj result = archStaticMetadataService.getArchivalStaticMetadata(id);

    assertNotNull(result);
    assertEquals("TestLob", result.getLobId());
    assertEquals("TestMetadataField", result.getStorageMetadataField());
    assertEquals("TestFieldType", result.getStorageFieldType());
    assertEquals("TestLookupValue", result.getLookupValue());
    assertEquals("TestLkpId", result.getLookupId());
}