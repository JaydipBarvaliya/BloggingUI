@Configuration
public class DelegateConfig {

    @Bean
    public ArchivalApiDelegate archivalApiDelegate(
            @Qualifier("archivalApiDelegateImpl") ArchivalApiDelegate impl) {
        return impl; // This one will go to ArchivalApiController
    }

    @Bean(name = "archivalApiMetadataDelegate")
    public ArchivalApiDelegate archivalApiMetadataDelegate(
            @Qualifier("archivalApiMetadataDelegateImpl") ArchivalApiDelegate impl) {
        return impl; // This one will go to a different controller
    }
}