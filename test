@Test
void test_updateConfig_shouldThrowExceptionWhenConfigNotFound() {
    String id = "999";
    ArchivalFileNetConfigObj input = new ArchivalFileNetConfigObj();

    when(filenetConfigRepository.findById(Long.valueOf(id)))
        .thenReturn(Optional.empty());

    SharedServiceLayerException exception = assertThrows(
        SharedServiceLayerException.class,
        () -> service.updateConfig(id, input)
    );

    assertTrue(exception.getMessage().contains("Configuration data not found to update"));
}