@Test
public void getClientApps_NotFoundTest() {
    String id = "client123";

    // Simulate service returning null
    Mockito.when(clientAppsApiService.getClientAppConfigData(id)).thenReturn(null);

    ResponseEntity<ClientAppsObj> response = clientAppsApiDelegate.getClientApps(id);

    Assertions.assertEquals(HttpStatus.NOT_FOUND, response.getStatusCode());
}