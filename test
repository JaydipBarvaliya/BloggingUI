import com.td.besig.opsapi.constants.HttpHeadersConstants;
import org.junit.jupiter.api.Test;

import java.lang.reflect.Constructor;
import java.lang.reflect.InvocationTargetException;

import static org.junit.jupiter.api.Assertions.*;

class HttpHeadersConstantsTest {

    @Test
    void testConstants() {
        assertEquals("Authorization", HttpHeadersConstants.AUTHORIZATION_HEADER);
        assertEquals("Content-Type", HttpHeadersConstants.CONTENT_TYPE_HEADER);
    }

    @Test
    void testPrivateConstructor() throws Exception {
        Constructor<HttpHeadersConstants> constructor =
                HttpHeadersConstants.class.getDeclaredConstructor();
        constructor.setAccessible(true);
        assertNotNull(constructor.newInstance());
    }
}