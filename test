@Test
public void getArchivalStaticMetadataListTest() {
    ArchStaticMetadataLookup obj = new ArchStaticMetadataLookup();
    obj.setLob("TestLob");
    obj.setArcStorageMetadataField("TestStorageMetadataFld");
    obj.setArcStorageFieldType("TestStorageFldType");
    obj.setLookupValue("TestLookupValue");
    obj.setLookupId("TestLkpId");

    List<ArchStaticMetadataLookup> configList = new ArrayList<>();
    configList.add(obj);

    Specification<ArchStaticMetadataLookup> spec = ArchStaticMetaDataSpec.dynamicQuery("testLob");

    when(repo.findAll(spec)).thenReturn(configList);

    List<ArchivalStaticMetadataObj> result = archStaticMetadataService.getArchivalStaticMetadataList("testLob");

    // Assert that the conversion and values are preserved
    assertNotNull(result);
    assertEquals(1, result.size());

    ArchivalStaticMetadataObj dto = result.get(0);
    assertEquals("TestLob", dto.getLobId());
    assertEquals("TestStorageMetadataFld", dto.getStorageMetadataField());
    assertEquals("TestStorageFldType", dto.getStorageFieldType());
    assertEquals("TestLookupValue", dto.getLookupValue());
    assertEquals("TestLkpId", dto.getLookupId());
}